<div class= "card text-center shadow-lg">
<ul id="events-list" >
    {{#each events}}
    <li data-id="{{this.id}}" class="list-group-item">
        <h5 class="card-header bg-warning shadow-lg"><strong>Event Name : </strong><a id="namelink" href="event/{{this.id}}">{{this.name}}</a></h5>
        {{!-- <p><strong>Date and Time :</strong> {{this.dateAndTime}}</p> --}}
        <div class="card-body">
        <h5 class="card-text" > <strong>Date :</strong> {{this.eventDate}}</p>
        <p class="card-text"><strong>Time :</strong> {{this.time}}</p>
        <p class="card-text"><strong >Host :</strong> {{this.fullname}}</p>

        <button class="btn btn-dark shadow-lg"><ahref="event/{{this.id}}" id="moreEventBtn">
                More about this Event</a></button>
    </li>
       {{/each}}
</ul>
</div>
