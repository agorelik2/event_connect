<br>
<br>
<button class="btn btn-primary "><a href="/newevent" id="newEventBtn">
        Create a New Event</a></button>

<br>
<br>

<ul id="events-list">
    {{#each events}}
    <li data-id="{{this.id}}" class="list-group-item">
        <p><strong>Event Name : </strong><a id="namelink" href="event/{{this.id}}">{{this.name}}</a></p>
        {{!-- <p><strong>Date and Time :</strong> {{this.dateAndTime}}</p> --}}
        <p><strong>Date :</strong> {{this.date}}</p>
        <p><strong>Time :</strong> {{this.time}}</p>
        <p><strong>Host :</strong> {{this.fullname}}</p>

        <button class="btn btn-primary "><a href="event/{{this.id}}" id="moreEventBtn">
                More about this Event</a></button>
    </li>
    {{/each}}
</ul>
<script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>

<script src="/js/members.js"></script>script>